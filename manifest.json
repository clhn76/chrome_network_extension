{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "A basic Chrome extension",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["webRequest", "declarativeNetRequest"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background/index.js",
    "type": "module"
  },
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset",
      "enabled": true,
      "path": "rules.json"
    }]
  }
}
